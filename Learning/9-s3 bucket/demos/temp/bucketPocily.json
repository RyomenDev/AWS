{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "blockallpublicaccess",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::aws-learnapp1/*",
                "arn:aws:s3:::aws-learnapp1",
            ],
            "Condition": {
                "StringNotEquals": {
                    "aws:PrincipalArn": "\"aws:PrincipalArn\":\"arn:aws:iam::AWS_ACCOUNT_ID:root"
                }
            }
        }
    ]
}